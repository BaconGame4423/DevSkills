{
  "default": {
    "cli": "opencode",
    "model": "zai-coding-plan/glm-5"
  },
  "overrides": {
    "specify": {
      "cli": "claude",
      "model": "opus"
    }
  },
  "gates": {},
  "auto_approve": true,
  "polling": {
    "interval": 1,
    "idle_timeout": 300,
    "max_timeout": 600,
    "step_timeouts": {
      "plan": {
        "idle_timeout": 300,
        "max_timeout": 1200
      },
      "implement": {
        "idle_timeout": 900,
        "max_timeout": 2400
      }
    }
  },
  "protected_files": [],
  "fallback_model": "zai-coding-plan/glm-5",
  "command_variant": "simple",
  "review_mode": "bash",
  "parallel": {
    "enabled": true,
    "strategy": "phase-split"
  }
}
